MDScreen:
    name: 'engineering'
    MDToolbar:
        title: 'Engineering'
        elevation: 10
        pos_hint : {"center_x":0.5, "center_y": 0.93}
        MDIconButton:
            icon: 'logout'
            pos_hint : {"center_x":0.9, "center_y": 0.5}
            on_press: root.manager.current = 'pcm'
    ScrollView:
        pos_hint : {"center_x":0.5, "center_y": 0.38}
        MDList:
            OneLineListItem:
                text: 'Engineering Branches'
                on_press: root.manager.current = 'engineering_branches'
            OneLineListItem:
                text: 'Integrated Engineering Programs'
                on_press: root.manager.current = 'integrated_engineering'
            OneLineListItem:
                text: 'National and State level Engineering Entrance Examinations'
                on_press: root.manager.current = 'engineering_entrance'