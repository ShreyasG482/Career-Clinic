MDScreen:
    name: 'diploma'
    MDFloatLayout:
        MDToolbar:
            title: 'Diploma'
            elevation: 10
            pos_hint : {"center_x":0.5, "center_y": 0.93}
            MDIconButton:
                icon: 'logout'
                pos_hint : {"center_x":0.9, "center_y": 0.5}
                on_press: root.manager.current = 'after_10'
        MDLabel:
            text: "Diploma is a very demanding course where you need not to do 12th and after successful completion of course you can directly take admission in second year Engineering if you want to."
            font_size: "20sp"
            pos_hint: {"center_x": .5, "center_y": .75}
            halign: 'center'
            color: rgba(255, 153, 51, 255)
        MDRectangleFlatButton :
            text : 'CLICK HERE for different Engineering Diplomas Fields'
            pos_hint : {"center_x":.5, "center_y": 0.5}
            on_press: root.manager.current = 'diplomas'