MDScreen:
    name: 'others'
    MDFloatLayout:
        MDToolbar:
            title: 'Others'
            elevation: 10
            pos_hint : {"center_x":0.5, "center_y": 0.93}
            MDIconButton:
                icon: 'logout'
                pos_hint : {"center_x":0.9, "center_y": 0.5}
                on_press: root.manager.current = 'explore'
        Button:
            text: "Choose your interest :"
            font_size: "20sp"
            size_hint: .75, .07
            pos_hint: {"center_x": 0.5, "center_y": 0.81}
            background_color: 0, 0, 0, 0
            canvas.before:
                Color:
                    rgb: (255/255, 80/255, 80/255, 1)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [20]
        Image:
